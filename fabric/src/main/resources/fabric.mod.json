{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "Vanilla Disable",
  "description": "${mod_description}",
  "authors": [
    "${mod_author}"
  ],
  "contributors": [
    "Wesley1808",
    "FallenBreath",
    "NoComment",
    "LittleLily",
    "ThatTrollzer",
    "Kichura",
    "ellieisjelly"
  ],
  "contact": {
    "sources": "https://github.com/DragonEggBedrockBreaking/VanillaDisable",
    "issues": "https://github.com/DragonEggBedrockBreaking/VanillaDisable/issues"
  },
  "license": "${license}",
  "icon": "assets/${mod_id}/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "uk.debb.vanilla_disable.config.global.VanillaDisableConfigLoader"
    ],
    "modmenu": [
      "uk.debb.vanilla_disable.config.global.ModMenuSettingsApi"
    ],
    "client": [
      "uk.debb.vanilla_disable.LangFileManager",
      "uk.debb.vanilla_disable.config.command.HotkeyManager"
    ]
  },
  "mixins": [
    "vanilla_disable.mixins.json",
    "vanilla_disable.fabric.mixins.json"
