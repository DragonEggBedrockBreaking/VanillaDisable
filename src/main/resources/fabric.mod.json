{
  "schemaVersion": 1,
  "id": "vanilla_disable",
  "version": "${version}",
  "name": "Vanilla Disable",
  "description": "Allows you to disable a lot of vanilla features.",
  "authors": [
    "DragonEggBedrockBreaking"
  ],
  "contributors": [
    "Wesley1808",
    "FallenBreath",
    "NoComment",
    "LittleLily",
    "ThatTrollzer"
  ],
  "contact": {
    "sources": "https://github.com/DragonEggBedrockBreaking/VanillaDisable",
    "issues": "https://github.com/DragonEggBedrockBreaking/VanillaDisable/issues"
  },
  "license": "MPL-2.0",
  "icon": "assets/vanilla_disable/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "uk.debb.vanilla_disable.config.global.VanillaDisableConfig",
      "uk.debb.vanilla_disable.data.gamerule.RegisterGamerules",
      "uk.debb.vanilla_disable.LangFileManager"
    ],
    "modmenu": [
      "uk.debb.vanilla_disable.config.global.ModMenuSettingsApi"
    ],
    "client": [
      "uk.debb.vanilla_disable.config.command.CommandConfigHotkeyManager"
    ]
  },
  "mixins": [
    "vanilla_disable.mixins.json",
    "vanilla_disable_conditional.mixins.json"
  ],
  "accessWidener": "vanilla_disable.aw",
  "depends": {
    "fabricloader": ">=0.14.9",
    "minecraft": "1.20.*",
    "java": ">=17"
  },
  "suggests": {
    "owo": "*"
  }
}
